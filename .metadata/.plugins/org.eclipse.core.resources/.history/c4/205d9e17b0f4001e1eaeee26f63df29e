package twoArray;

public class Shape {
	public static void main(String[] args) {
		for(int i = 1; i <= 5; i++) { //바깥for : i가 1부터 5까지 반복
	         for(int j = 1; j <= i; j++) { //안쪽for : j가 1부터 3까지 반복
//	            System.out.print("i = " + i + ", j = " + j);   
	            System.out.print("* ");//별표와 공백을 번갈아하면서 출력
	         }
	         System.out.println();
	      }
	}
}
